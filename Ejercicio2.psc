Proceso Ejercicio2
	
	Definir i, j, k Como Entero;
	
	i <- 1;

	Mientras i <= 10 Hacer
		
		j <- 1;
		k <- 1;
		Mientras j <= 10-i Hacer
			Escribir  " " Sin Saltar;
			j <- j+1;
		FinMientras

		Mientras k <= i Hacer
			Escribir "*" Sin Saltar;
			k <- k+1;
		FinMientras
		
		Escribir "";
		
		i <- i+1;
		
	FinMientras
	
FinProceso